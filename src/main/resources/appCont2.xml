<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">



    <bean id="player"
          class = "com.dorosh.springPractice.musicLessonL3-5.Player">
        <constructor-arg ref="rockMusic"/>
    </bean>

    <bean id = "classicMusic"
          class = "com.dorosh.springPractice.musicLessonL3-5.ClassicMusic">
    </bean>

    <bean id = "rockMusic"
          class = "com.dorosh.springPractice.musicLessonL3-5.RockMusic">
    </bean>






</beans>